{
  "name": "alexpagnotta.dev",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.17.1",
    "npm": ">=9.6.7"
  },
  "scripts": {
    "clean": "rimraf .next out node_modules/.cache",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "npm run build && npm start",
    "export": "npm run build && next export",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "ANALYZE=true npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --color --cache --cache-location 'node_modules/.cache/.eslintcache' app --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write 'app/**/*.{json,md,css,js,jsx,ts,tsx}' && npm run lint -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-ui/react-accessible-icon": "1.0.3",
    "@radix-ui/react-slot": "1.0.2",
    "@vercel/analytics": "1.0.2",
    "class-variance-authority": "0.7.0",
    "date-fns": "2.30.0",
    "esbuild": "0.19.2",
    "framer-motion": "10.16.4",
    "glob": "10.3.4",
    "gray-matter": "4.0.3",
    "mdx-bundler": "9.2.1",
    "next": "13.4.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-plock": "3.0.2",
    "spin-delay": "1.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.3.1",
    "@savvywombat/tailwindcss-grid-areas": "3.1.0",
    "@svgr/webpack": "8.0.1",
    "@tailwindcss/typography": "0.5.9",
    "@types/node": "20.4.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "postcss": "8.4.25",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6"
  }
}
