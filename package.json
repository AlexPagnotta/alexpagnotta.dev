{
  "name": "alexpagnotta.dev",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16.17.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "clean": "rimraf .next out node_modules/.cache",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "npm run build && npm start",
    "export": "npm run build && next export",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "ANALYZE=true npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --color --cache --cache-location 'node_modules/.cache/.eslintcache' app --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write 'app/**/*.{json,md,css,js,jsx,ts,tsx}' && npm run lint -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "13.4.9",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.3.1",
    "@types/node": "20.4.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "postcss": "8.4.25",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6"
  }
}
